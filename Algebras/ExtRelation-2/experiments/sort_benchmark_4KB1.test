#--------------------------------------------------  
# Enable Flags
#--------------------------------------------------  

set "ERA:TraceSort" = TRUE;
#set "SI:PrintCounters" = TRUE;
set "SI:PrintCmdTimes" = TRUE;
set "SI:NoQueryTimers" = TRUE;
set "SI:NoCommandEcho" = TRUE;
#set "SI:RelStatistics" = TRUE;
set "SMI:NoTransactions" = TRUE;

#--------------------------------------------------  
# Create and open database DB_R1G
#--------------------------------------------------  

#setup sort_benchmark_4KB StandardAlgebra RelationAlgebra ExtRelation2Algebra

create database db_R1G;
open database db_R1G;

#--------------------------------------------------  
# Sort 1 MB data
#--------------------------------------------------  

#testcase create relation
#yields success
restore R1MB_T4KB from '$(SECONDO_BUILD_DIR)/Algebras/ExtRelation-2/experiments/R1MB_T4KB';

#testcase Old sort algorithm - time for run generation
#yields success
query R1MB_T4KB feed sort head[1] count;

#testcase Old sort algorithm - total time
#yields success
query R1MB_T4KB feed sort count;

#testcase New sort2 algorithm
#yields success
query R1MB_T4KB feed sort2 count;

#testcase Old sortby algorithm - time for run generation
#yields success
query R1MB_T4KB feed sortby[Flob asc, Nr asc] head[1] count;

#testcase Old sortby algorithm - total time
#yields success
query R1MB_T4KB feed sortby[Flob asc, Nr asc] count;

#testcase New sortby2 algorithm
#yields success
query R1MB_T4KB feed sortby2[Flob asc, Nr asc] count;

#testcase Delete relation
#yields success
delete R1MB_T4KB;

