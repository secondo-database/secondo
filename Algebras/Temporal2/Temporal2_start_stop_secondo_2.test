#setup Forall2Test StandardAlgebra RelationAlgebra Temporal2Algebra

# Here come: The tests

#testcase close_update_open_db_9
#yields success
open database mytestdb;

# ------------------------------------------------------------------
# -- variable assignement - let - check after re-open and updates --
# ------------------------------------------------------------------

#testcase mmpoint_variable_undef_3
#yields (mmpoint undefined)
query mm_var_undef;

#testcase mmpoint_variable_2000_3
#yields (mmpoint((("2000-01-01" "2000-01-01-00:01" TRUE FALSE)(0.0 0.0 1.0 1.0))))
query mm_var_2000;

#testcase mmpoint_variable_2000_2100_3
#yields (mmpoint((("2000-01-01" "2000-01-01-00:01" TRUE FALSE)(0.0 0.0 1.0 1.0))(("2100-01-01" "2100-01-01-00:01" TRUE FALSE)(1.0 1.0 0.0 0.0))))
query mm_var_2000_2100;


# -------------------------------------------------------------
# -- attribute in relation - check after re-open and updates --
# -------------------------------------------------------------

#testcase mmpoint_relation_undef_3
#yields ((rel (tuple ((MMP mmpoint)))) ((undefined)))
query mm_rel_undef;

#testcase mmpoint_relation_2000_3
#yields ((rel(tuple((MMP mmpoint))))((((("2000-01-01" "2000-01-01-00:01" TRUE FALSE)(0.0 0.0 1.0 1.0))))))
query mm_rel_2000;

#testcase mmpoint_relation_2000_2100_3
#yields ((rel(tuple((MMP mmpoint))))((((("2000-01-01" "2000-01-01-00:01" TRUE FALSE)(0.0 0.0 1.0 1.0))(("2100-01-01" "2100-01-01-00:01" TRUE FALSE)(1.0 1.0 0.0 0.0))))))
query mm_rel_2000_2100;

#testcase mmpoint_relation_invalid
#yields error
let mm_rel_invalid = [const rel(tuple([MMP: mmpoint])) value (( 123 ))];


#teardown
close database;
delete database mytestdb;
delete database mytestdb2;