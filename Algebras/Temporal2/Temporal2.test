#testcase mmpoint_variable_undef_1
#yields success
let mm_var_undef = [const mmpoint value undef];

#testcase mmpoint_variable_undef_2
#yields (mmpoint undefined)
query mm_var_undef;


#testcase mmpoint_variable_2000_1
#yields success
let mm_var_2000 = [const mmpoint value ((("2000-01-01" "2000-01-01-00:01" TRUE FALSE)(0.0 0.0 1.0 1.0)))];

#testcase mmpoint_variable_2000_2
#yields (mmpoint((("2000-01-01" "2000-01-01-00:01" TRUE FALSE)(0.0 0.0 1.0 1.0))))
query mm_var_2000;


#testcase mmpoint_variable_2000_2100_1
#yields success
let mm_var_2000_2100 = [const mmpoint value ((("2000-01-01" "2000-01-01-00:01" TRUE FALSE)(0.0 0.0 1.0 1.0))(("2100-01-01" "2100-01-01-00:01" TRUE FALSE)(1.0 1.0 0.0 0.0)))];

#testcase mmpoint_variable_2000_2100_2
#yields (mmpoint((("2000-01-01" "2000-01-01-00:01" TRUE FALSE)(0.0 0.0 1.0 1.0))(("2100-01-01" "2100-01-01-00:01" TRUE FALSE)(1.0 1.0 0.0 0.0))))
query mm_var_2000_2100;


#testcase mmpoint_variable_invalid
#yields error
let mm_var_invalid = [const mpoint value 123];

